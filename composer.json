{
    "name": "blackpig-creatif/sceau",
    "description": "SEO metadata management plugin for FilamentPHP v4",
    "keywords": [
        "laravel",
        "filament",
        "seo",
        "meta tags",
        "structured data",
        "json-ld"
    ],
    "license": "MIT",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Blackpig/chambre-noir"
        }
    ],
    "require": {
        "php": "^8.2",
        "blackpig-creatif/chambre-noir": "^1.0|dev-main",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^11.0|^12.0",
        "lara-zeus/spatie-translatable": "^1.0",
        "spatie/laravel-package-tools": "^1.16",
        "spatie/laravel-translatable": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "BlackpigCreatif\\Sceau\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlackpigCreatif\\Sceau\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackpigCreatif\\Sceau\\SceauServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
